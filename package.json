{
  "name": "pizza_game",
  "version": "1.0.0",
  "description": "A simple 2 player pizza game",
  "main": "lib/index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
